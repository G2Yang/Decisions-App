<?xml version="1.0" encoding="utf-8"?>
<MotionScene xmlns:tools="http://schemas.android.com/tools"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:motion="http://schemas.android.com/apk/res-auto"
    >

    <Transition
        motion:constraintSetEnd="@+id/end"
        motion:constraintSetStart="@id/start"
        motion:duration="3000"
        motion:autoTransition="animateToEnd">
        <KeyFrameSet>

            <KeyAttribute
                motion:motionTarget="@+id/circleBack"
                motion:framePosition="0"
                android:scaleX="0" />
            <KeyAttribute
                motion:motionTarget="@+id/circleBack"
                motion:framePosition="0"
                android:scaleY="0" />
            <KeyAttribute
                motion:motionTarget="@+id/circleBack"
                motion:framePosition="100"
                android:scaleX="7" />
            <KeyAttribute
                motion:motionTarget="@+id/circleBack"
                motion:framePosition="100"
                android:scaleY="7" />
            <KeyAttribute
                motion:motionTarget="@+id/circleBack"
                motion:framePosition="20"
                android:scaleX="1" />
            <KeyAttribute
                motion:motionTarget="@+id/circleBack"
                motion:framePosition="20"
                android:scaleY="1" />
            <KeyAttribute
                motion:motionTarget="@+id/circleBack"
                motion:framePosition="60"
                android:scaleX="1" />
            <KeyAttribute
                motion:motionTarget="@+id/circleBack"
                motion:framePosition="60"
                android:scaleY="1" />
            <KeyAttribute
                motion:motionTarget="@+id/circleBack"
                motion:framePosition="40"
                android:scaleX="1" />
            <KeyAttribute
                motion:motionTarget="@+id/circleBack"
                motion:framePosition="40"
                android:scaleY="1" />
            <KeyAttribute
                motion:motionTarget="@+id/circleBack"
                motion:framePosition="80"
                android:scaleX="1" />
            <KeyAttribute
                motion:motionTarget="@+id/circleBack"
                motion:framePosition="80"
                android:scaleY="1" />
            <KeyAttribute />
        </KeyFrameSet>



    </Transition>

    <ConstraintSet android:id="@+id/start">
        <Constraint
            android:id="@+id/circleBack"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="250dp"
            android:layout_height="250dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintStart_toStartOf="parent" />
        <Constraint
            android:id="@+id/logoDecisions"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="240dp"
            android:layout_height="150dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="50dp"
            motion:layout_constraintVertical_bias="0.5"
            motion:layout_constraintHorizontal_bias="0.5"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            android:rotationY="0" />
        <Constraint
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintRight_toRightOf="parent"
            motion:layout_constraintLeft_toLeftOf="parent"
            android:id="@+id/titleName"
            motion:layout_constraintTop_toBottomOf="@+id/logoDecisions"
            motion:layout_constraintVertical_bias="0.25" />
    </ConstraintSet>

    <ConstraintSet android:id="@+id/end">
        <Constraint
            android:id="@+id/circleBack"
            android:layout_width="300dp"
            android:layout_height="300dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toTopOf="parent">


        </Constraint>
        <Constraint
            android:id="@+id/logoDecisions"
            android:layout_width="240dp"
            android:layout_height="150dp"
            android:layout_marginBottom="50dp"
            android:rotationY="360"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintHorizontal_bias="0.5"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintVertical_bias="0.5"
            motion:pathMotionArc="flip"
            motion:transitionEasing="accelerate" />
    </ConstraintSet>
</MotionScene>